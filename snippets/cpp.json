{
	"Add File Header": {
		"prefix": "fileheader",
		"body": [
			"/**",
			" * @file    :${TM_FILENAME}",
			" * @brief   :$1",
			" * @version :V1.0.0",
			" * @date    :${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}",
			" */\n"
		],
		"description": "Add file describe"
	},
	"pragma once":{
		"prefix": "#pragma",
		"body": [
			"#pragma once\n\n"
		]
	},
	"snippets: #inc": {
		"prefix": "#inc",
		"body": [
			"#include \"$1\""
		],
		"description": "Code snippet for #include \" \""
	},
	"snippets: #inc<>": {
		"prefix": "#incj",
		"body": [
			"#include <$1>"
		],
		"description": "Code snippet for #include \" \""
	},
	"snippets: #ifndef _TM_FILENAME_BASE_H": {
		"prefix": "#ifndef_header",
		"body": [
			"#ifndef _${1:${TM_FILENAME_BASE/(.*)/${1:/upcase}/}}_H",
			"#define _$1_H",
			"$2\n",
			"#endif\t// _$1_H"
		],
		"description": "Code snippet for #ifdef _XXX_H"
	},
	"snippets: #def": {
		"prefix": "#def",
		"body": [
			"#define \"$1\" \"$2\" "
			],
		"description": "Code snippet for #define \" \""
	},
	"snippets: #ifdef": {
		"prefix": "#ifdef",
		"body": [
			"#ifdef ${_DEBUG}",
			"$1",
			"#endif // ${_DEBUG}"
		],
		"description": "Code snippet for #ifdef"
	},
	"snippets: #ifndef": {
		"prefix": "#ifndef",
		"body": [
			"#ifndef ${1:1}",
			"$2",
			"#endif // !$1"
		],
		"description": "Code snippet for #ifndef"
	},
	"snippets: #if": {
		"prefix": "#if",
		"body": [
			"#ifdef ${1:0}",
			"$2",
			"#endif // $1"
		],
		"description": "Code snippet for #if"
	},
	"snippets: int main()": {
		"prefix": "main",
		"body": [			
			"#include <iostream>",
			"//using namespace std;",
			"",
			"$2",
			"",
			"int main()",
			"{",
				"\t$1",
				"\t",
				"\tsystem(\"pause\");",
				"\treturn 0;",
			"}"
		],
		"description": "Code snippet for main function"
	},
	"snippets: int main(int argc, char* argv[])": {
		"prefix": "main",
		"body": [
			"#include <iostream>",
			"//using namespace std;",
			"",
			"$2",
			"",
			"int main(int argc, char* argv[])",
			"{",
			"\t$1",
			"\t",
			"\tsystem(\"pause\");",
			"\treturn 0;",
			"}"
		],
		"description": "Code snippet for main function"
	},
	"snippets: void fun(){}": {
		"prefix": "void_function",
		"body": [
			"void ${1:FunctionName}(${2:/* args */})",
			"{",
			"\t${3:/* Code */}",
			"}"
		],
		"description": "Code snippet for void functionname(args){}"
	},
	"snippets: fun(){}": {
		"prefix": "function",
		"body": [
			"${1:type} ${2:FunctionName}(${3:/* args */})",
			"{",
			"\t${4:/* Code */}",
			"}"
		],
		"description": "Code snippet for functionname(args){}"
	},
	"snippets: Class Object": {
		"prefix": "classh",
		"body": [
			"#ifndef _${1:${TM_FILENAME_BASE/(.*)/${1:/upcase}/}}_H",
			"#define _$1_H ",
			" ",
			"class ${2:ClassName}",
			"{",
			"public:",
			"\t${2:ClassName}();",
			"\t~${2:ClassName}();",
			"\t${4:/* functions */}",
			"private:",
			"\t${3:/* data */}",
			"};",
			"#endif\t// _$1_H"
		],
		"description": "class.h"
	},
	"snippets: comment_block": {
